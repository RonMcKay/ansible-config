# {{ ansible_managed }}

if [ -f ~/.sh_aliases ]; then
    source ~/.sh_aliases
fi

if [ -f ~/.sh_environment ]; then
    source ~/.sh_environment
fi

if [[ "$SHELL" == */bash ]] && [[ -f ~/.fzf.bash ]]; then
    source ~/.fzf.bash
elif [[ "$SHELL" == */zsh ]] && [[ -f ~/.fzf.zsh ]]; then
    source ~/.fzf.zsh
fi

umask 022
